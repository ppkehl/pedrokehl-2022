---
import Base from '../../../layouts/Base.astro'

export async function getStaticPaths() {

const posts = await Astro.glob('../../../data/test/**/')
const paths = posts.map(post => {

  return {
    params: {
      slug: post.frontmatter.slug
    },
    props: {
      post
    }
  }
})
return paths;
}
